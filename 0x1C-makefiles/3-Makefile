# create executable

CC=gcc
SRC=main.c school.c
OBJ=main.o school.o
NAME=school
RM=rm -f

all: $(NAME)

$(NAME) : $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

main.o: main.c m.h
	$(CC) -c -o main.o main.c

school.o: school.c m.h
	$(CC) -c -o school.o school.c

.PHONY: fclean clean oclean

fclean: clean oclean

clean:
	$(RM) *~ $(NAME) 

oclean:
	$(RM) $(OBJ)

.PHONY: re

re : oclean all
